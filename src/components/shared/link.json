{
  "collectionName": "components_shared_links",
  "info": {
    "displayName": "Link"
  },
  "options": {},
  "attributes": {
    "href": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "isExternal": {
      "type": "boolean",
      "default": false
    },
    "target": {
      "type": "enumeration",
      "enum": ["_self", "_blank"],
      "default": "_self"
    },
    "rel": {
      "type": "enumeration",
      "enum": ["nofollow", "noopener", "noreferrer"],
      "default": "noopener"
    },
    "ariaLabel": {
      "type": "string"
    },
    "isButtonLink": {
      "type": "boolean",
      "default": false
    },
    "type": {
      "type": "enumeration",
      "enum": ["primary", "secondary", "outline", "ghost", "danger", "success"]
    },
    "size": {
      "type": "enumeration",
      "enum": ["small", "medium", "large"],
      "default": "medium"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "iconPosition": {
      "type": "enumeration",
      "enum": ["left", "right", "none"],
      "default": "none"
    },
    "animation": {
      "type": "enumeration",
      "enum": ["none", "fadeIn", "slideUp", "bounce"],
      "default": "none"
    },
    "showOnMobile": {
      "type": "boolean",
      "default": true
    },
    "showOnTablet": {
      "type": "boolean",
      "default": true
    },
    "showOnDesktop": {
      "type": "boolean",
      "default": true
    },
    "className": {
      "type": "string"
    }
  },
  "config": {}
}
